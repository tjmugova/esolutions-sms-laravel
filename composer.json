{
    "name": "tjmugova/esolutions-sms-laravel",
    "description": "Provides EsolutionsSms notification channel for Laravel",
    "license": "MIT",
    "authors": [
      {
        "name": "Tererai Mugova",
        "email": "tjmugova@webstudio.co.zw"
      }
    ],
    "keywords": [
      "laravel-sms",
      "notifications",
      "esolutions"
    ],
    "require": {
      "php": "^8.2",
      "illuminate/notifications": "^11.0 || ^12.0",
      "illuminate/support": "^11.0 || ^12.0",
      "illuminate/events": "^11.0 || ^12.0",
      "illuminate/queue": "^11.0 || ^12.0"
    },
    "autoload": {
      "psr-4": {
        "Tjmugova\\EsolutionsSms\\": "src",
        "Tjmugova\\EsolutionsSms\\Tests\\": "tests"
      },
      "files": []
    },
    "extra": {
      "laravel": {
        "providers": [
          "Tjmugova\\EsolutionsSms\\EsolutionsSmsServiceProvider"
        ],
        "aliases": {
          "EsolutionsSms": "Tjmugova\\EsolutionsSms\\EsolutionsSms"
        }
      }
    },
    "require-dev": {
      "orchestra/testbench": "^6.0",
      "phpunit/phpunit": "^9.5"
    }
  }